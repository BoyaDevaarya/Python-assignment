   "source": [
    "def mySqrt(x):\n",
    "    i = 0\n",
    "    while i * i <= x:\n",
    "        i += 1\n",
    "    return i - 1\n",
    "\n",
    "# Test\n",
    "print(mySqrt(4))   # 2\n",
    "print(mySqrt(8))   # 2\n"
   ]
  }
